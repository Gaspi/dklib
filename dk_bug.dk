#NAME dk_bug

type : Type.
e : type -> Type.

P : Type.
eP : P -> Type.

(; equality ;)
equal : A : type -> x : e A -> y : e A -> P.
refl : A : type -> x : e A -> eP (equal A x x). 

equal_ind : A : type ->
            P : (e A -> type) ->
            x : e A ->
            y : e A ->
            eP (equal A x y) ->
            e (P x) ->
            e (P y).

[A : type,
 P : e A -> type,
 x : e A,
 p : e (P x)]
  equal_ind _ P x _ (refl A _) p --> p.
